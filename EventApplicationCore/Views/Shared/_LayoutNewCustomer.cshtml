    @Html.Raw(JavaScriptSnippet.FullScript)
</head>
<body class="hold-transition layout-top-nav">
    <div class="wrapper">
        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand-md navbar-light navbar-white">
            <div class="container">
                <a href="#" class="navbar-brand">
                    @*<img src="~/images/logo.png" alt="Eventify Logo" class="brand-image img-circle elevation-3" style="opacity: .8">*@
                    <i class="fa fa-calendar"></i>
                    <span class="brand-text font-weight-light">Eventify</span>
                </a>

                <button class="navbar-toggler order-1" type="button" data-toggle="collapse" data-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse order-3" id="navbarCollapse">
                    <!-- Left navbar links -->
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a class="nav-link" asp-controller="Customer" asp-action="Dashboard" class="navbar-brand">Dashboard</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-controller="Booking" asp-action="BookingVenue">Book Event</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-controller="ShowAllBooking" asp-action="AllBookings">Booking Status</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-controller="UserProfile" asp-action="Profile">Profile</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-controller="Customer" asp-action="ChangePassword">ChangePassword</a>
                        </li>
                    </ul>
                </div>

                <!-- Right navbar links -->
                <ul class="order-1 order-md-3 navbar-nav navbar-no-expand ml-auto">
                    <li class="nav-link">

                    </li>
                    <!-- SEARCH FORM -->
                    <form class="form-inline ml-0 ml-md-3">
                        <div class="input-group input-group-sm">
                            <input class="form-control form-control-navbar" type="search" placeholder="Search" aria-label="Search">
                            <div class="input-group-append">
                                <button class="btn btn-navbar" type="submit">
                                    <i class="fas fa-search"></i>
                                </button>
                            </div>
                        </div>
                    </form>
                </ul>
            </div>
        </nav>
        <!-- /.navbar -->
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <div class="content">
            @RenderBody()
            </div>
        </div>
        <!-- /.content-wrapper -->
        
        <!-- Main Footer -->
        <footer class="main-footer">
            <!-- To the right -->
            @*<div class="float-right d-none d-sm-inline">
                Anything you want
            </div>*@
            <!-- Default to the left -->
            <strong>Copyright &copy; @DateTime.Now.Year <a href="#">Eventify</a>.</strong> All rights reserved.
        </footer>
        @*<div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
        <p>&copy; 2017 - EventApplicationCore</p>
        </footer>
        </div>*@
    </div>
    <!-- ./wrapper -->
    <!-- jQuery -->
    <script src="~/frontEnd/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap 4 -->
    <script src="~/frontEnd/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- DataTables  & Plugins -->
    <script src="~/frontEnd/plugins/datatables/jquery.dataTables.min.js"></script>
    <script src="~/frontEnd/plugins/datatables-bs4/js/dataTables.bootstrap4.min.js"></script>
    <script src="~/frontEnd/plugins/datatables-responsive/js/dataTables.responsive.min.js"></script>
    <script src="~/frontEnd/plugins/datatables-responsive/js/responsive.bootstrap4.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/frontEnd/dist/js/adminlte.min.js"></script>

    <environment names="Staging,Production">
